<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	  <servlet>
	    <servlet-name>ArtistServlet</servlet-name>
	    <servlet-class>com.google.developers.schemas.music_actions.ArtistServlet</servlet-class>
	  </servlet>
	  <servlet>
	    <servlet-name>AlbumServlet</servlet-name>
	    <servlet-class>com.google.developers.schemas.music_actions.AlbumServlet</servlet-class>
	  </servlet>
	  <servlet>
	    <servlet-name>SearchServlet</servlet-name>
	    <servlet-class>com.google.developers.schemas.music_actions.SearchServlet</servlet-class>
	  </servlet>
	  <servlet>
	    <servlet-name>TrackServlet</servlet-name>
	    <servlet-class>com.google.developers.schemas.music_actions.TrackServlet</servlet-class>
	  </servlet>
	  <servlet>
	  	<servlet-name>TestDataServlet</servlet-name>
	  	<servlet-class>com.google.developers.schemas.music_actions.TestDataServlet</servlet-class>
	  	<load-on-startup>1</load-on-startup>
	  </servlet>
	  <servlet-mapping>
	    <servlet-name>ArtistServlet</servlet-name>
	    <url-pattern>/artist/*</url-pattern>
	  </servlet-mapping>
	  <servlet-mapping>
	    <servlet-name>AlbumServlet</servlet-name>
	    <url-pattern>/album/*</url-pattern>
	  </servlet-mapping>
	  <servlet-mapping>
	    <servlet-name>SearchServlet</servlet-name>
	    <url-pattern>/search</url-pattern>
	    <url-pattern>/search.json</url-pattern>
	  </servlet-mapping>
	  <servlet-mapping>
	    <servlet-name>TrackServlet</servlet-name>
	    <url-pattern>/track/*</url-pattern>
	  </servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
